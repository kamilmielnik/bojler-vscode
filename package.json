{
  "name": "bojler-vscode",
  "version": "0.0.1",
  "displayName": "Bojler",
  "description": "Bojler Extension for Visual Studio Code",
  "main": "./extension",
  "categories": [
    "Other"
  ],
  "keywords": [
    "boil",
    "boiler",
    "boilerplate",
    "generator",
    "snippets"
  ],
  "engines": {
    "node": ">=16.0.0",
    "vscode": "^1.72.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/bojler-vscode.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil@kamilmielnik.com",
    "url": "https://kamilmielnik.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamilmielnik/bojler-vscode/issues"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Bojler",
        "command": "extension.bojler"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "extension.bojler",
          "group": "explorer-context-menu-command"
        }
      ]
    }
  },
  "dependencies": {
    "bojler": "^1.0.0"
  }
}
